# http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[targets/**/fixtures/*]
insert_final_newline = false

[**/http1.1/fixtures/*]
end_of_line = crlf
insert_final_newline = false

[**/http1.1/fixtures/jsonObj-multiline]
end_of_line = unset

[*.md]
trim_trailing_whitespace = false
